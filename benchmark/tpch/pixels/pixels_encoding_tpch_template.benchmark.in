# name: benchmark/tpch/parquet/pixels_q${QUERY_NUMBER_PADDED}.benchmark
# description: Execute Q${QUERY_NUMBER_PADDED} over lineitem stored in a pixels file
# group: [pixels]

name Q${QUERY_NUMBER_PADDED} (Pixels)
group pixels
subgroup tpch

require pixels
require tpch

load
CREATE VIEW lineitem AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/lineitem/v-0-ordered/*.pxl');
CREATE VIEW nation AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/nation/v-0-ordered/*.pxl');
CREATE VIEW region AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/region/v-0-ordered/*.pxl');
CREATE VIEW supplier AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/supplier/v-0-ordered/*.pxl');
CREATE VIEW customer AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/customer/v-0-ordered/*.pxl');
CREATE VIEW part AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/part/v-0-ordered/*.pxl');
CREATE VIEW partsupp AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/partsupp/v-0-ordered/*.pxl');
CREATE VIEW orders AS SELECT * FROM pixels_scan('/home/liyu/tpch-data/pixels_data/tpch-${SF_NUMBER_UNDERSCORE}-encoding/orders/v-0-ordered/*.pxl');

run extension/tpch/dbgen/queries/q${QUERY_NUMBER_PADDED}.sql

result extension/tpch/dbgen/answers/sf${SF_NUMBER}/q${QUERY_NUMBER_PADDED}.csv