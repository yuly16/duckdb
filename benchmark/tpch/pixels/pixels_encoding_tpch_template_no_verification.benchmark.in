# name: benchmark/tpch/parquet/pixels_q${QUERY_NUMBER_PADDED}.benchmark
# description: Execute Q${QUERY_NUMBER_PADDED} over lineitem stored in a pixels file
# group: [pixels]

name Q${QUERY_NUMBER_PADDED} (Pixels)
group pixels
subgroup tpch

require pixels
require tpch

load
CREATE VIEW lineitem AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/lineitem/v-0-compact/*.pxl');
CREATE VIEW nation AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/nation/v-0-compact/*.pxl');
CREATE VIEW region AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/region/v-0-compact/*.pxl');
CREATE VIEW supplier AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/supplier/v-0-compact/*.pxl');
CREATE VIEW customer AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/customer/v-0-compact/*.pxl');
CREATE VIEW part AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/part/v-0-compact/*.pxl');
CREATE VIEW partsupp AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/partsupp/v-0-compact/*.pxl');
CREATE VIEW orders AS SELECT * FROM pixels_scan('/data/s1725-2/liyu/pixels_data/pixels-tpch-${SF_NUMBER_UNDERSCORE}-encoded/orders/v-0-compact/*.pxl');

run extension/tpch/dbgen/queries/q${QUERY_NUMBER_PADDED}.sql

