# name: benchmark/tpch/parquet/parquet_q1.benchmark
# description: Execute Q1 over lineitem stored in a parquet file
# group: [parquet]

name Q1 (Pixels)
group pixels
subgroup tpch

require pixels
require tpch

load
CREATE VIEW lineitem AS SELECT * FROM pixels_scan('/scratch/liyu/opt/pixels_file/pixels-tpch-300/lineitem/v-0-order/*.pxl');

run
PRAGMA tpch(1)

